{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
    "consistent-return": 0,
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0
  }
}
